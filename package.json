{
  "name": "webpack-babel-starter",
  "version": "0.0.1",
  "description": "Webpack Babel Starter Kit",
  "scripts": {
    "start": "npm run webpack",
    "test": "npm run lint",
    "lint": "./node_modules/.bin/eslint src",
    "clean-build": "node ./bin/clean-build.js",
    "build": "npm run clean-build && ./node_modules/.bin/webpack --progress",
    "build-prod": "npm run clean-build && NODE_ENV=production ./node_modules/.bin/webpack --progress -p",
    "webpack": "./node_modules/.bin/webpack-dev-server --progress --colors --hot --inline"
  },
  "author": "Christophe Rosset <tophe@topheman.com> (http://labs.topheman.com/)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "git-rev-sync": "^1.4.0",
    "html-webpack-plugin": "^2.9.0",
    "json-loader": "^0.5.4",
    "moment": "^2.11.2",
    "node-sass": "^3.4.2",
    "npmlog": "^2.0.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
